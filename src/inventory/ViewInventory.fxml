<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="1000" prefWidth="1600" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="inventory.ViewInventoryController">
    <TableView fx:id="tableView" layoutX="20.0" layoutY="20.0" prefHeight="560.0" prefWidth="860.0">
        <columns>
            <TableColumn fx:id="idColumn" prefWidth="50.0" text="ID" />
            <TableColumn fx:id="itemNameColumn" prefWidth="150.0" text="Item Name" />
            <TableColumn fx:id="descriptionColumn" prefWidth="250.0" text="Description" />
            <TableColumn fx:id="quantityColumn" prefWidth="100.0" text="Quantity" />
            <TableColumn fx:id="lastUpdatedColumn" prefWidth="150.0" text="Last Updated" />
        </columns>
    </TableView>
</AnchorPane>
